{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run test file",
			"type": "shell",
			"command": "${config:python.pythonPath} -B ${workspaceFolder}/wrappers/python/test.py",
			"problemMatcher": {
				"owner": "python",
				"fileLocation": "autoDetect",
				"pattern": [
						{
								"regexp": "^.*File \\\"([^\\\"]|.*)\\\", line (\\d+).*",
								"file": 1,
								"line": 2
						},
						{
								"regexp": "^.*raise.*$"
						},
						{
								"regexp": "^\\s*(.*)\\s*$",
								"message": 1
						}
				]
		}
	},
	]
}